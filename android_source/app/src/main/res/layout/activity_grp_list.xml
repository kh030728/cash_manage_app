<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="inu.dbproj.GrpListActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textview_cash_activity_grp_list"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="  원"
            android:textSize="25dp"
            android:layout_margin="10dp"
            android:textColor="@android:color/black"
            android:gravity="right"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="모임 소속 인원"
            android:textSize="25dp"
            android:textColor="@android:color/black"
            android:background="#AA88FF"
            android:gravity="center"/>
        <ExpandableListView
            android:id="@+id/exlist_userlist_activity_grp_list"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"></ExpandableListView>

    </LinearLayout>




</android.support.constraint.ConstraintLayout>
